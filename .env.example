# HubSpot Document Intelligence App Configuration
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3000
NODE_ENV=development
APP_BASE_URL=http://localhost:3000

# HubSpot OAuth Credentials
# Get these from your HubSpot developer account: https://developers.hubspot.com/
HUBSPOT_CLIENT_ID=your_client_id_here
HUBSPOT_CLIENT_SECRET=your_client_secret_here
HUBSPOT_REDIRECT_URI=http://localhost:3000/oauth/callback

# OpenAI API Key
# Get this from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Timeline Event Template ID
# Create this in your HubSpot developer account
HUBSPOT_TIMELINE_EVENT_TEMPLATE_ID=
